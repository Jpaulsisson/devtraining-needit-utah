<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Sets the options for the "What needed" field based on the current value of the Request Type field</description>
        <field>u_request_type</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Set "What needed" option by Request Type</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }
   if (newValue === 'hr') {
	g_form.removeOption('what_needed', 'facilities1');
	g_form.removeOption('what_needed', 'facilities2');
	g_form.removeOption('what_needed', 'legal1'); 
	g_form.removeOption('what_needed', 'legal2');
   }
	else if (newValue === 'facilities') {
	g_form.removeOption('what_needed', 'hr1');
	g_form.removeOption('what_needed', 'hr2');
	g_form.removeOption('what_needed', 'legal1'); 
	g_form.removeOption('what_needed', 'legal2');
   }
	else if (newValue === 'legal') {
	g_form.removeOption('what_needed', 'facilities1');
	g_form.removeOption('what_needed', 'facilities2');
	g_form.removeOption('what_needed', 'hr1'); 
	g_form.removeOption('what_needed', 'hr2');
   }
}]]></script>
